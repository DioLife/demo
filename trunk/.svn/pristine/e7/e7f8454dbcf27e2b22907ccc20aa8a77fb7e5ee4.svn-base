import UIKit
import HandyJSON

class BcLotteryOrder: HandyJSON {

    required init(){}
    /**
     * 订单号
     */
    var orderId:String!;
    
    /**
     * 会员账号名
     */
    var account:String!;
    var accountId:Int32!;
    /**
     * 彩票编号
     */
    var lotCode:String!;
    /**
     * 彩种名称
     *
     * @return
     */
    var lotName:String!;
    
    
    /**
     * 购买的号码
     */
    var haoMa:String!;
    
    /**
     * 购买时间
     */
    var createTime:Int64!;
    
    /**
     * 购买金额
     */
    var buyMoney:Float!;
    
    /**
     * 中奖金额
     */
    var winMoney:Float!;
    
    /**
     * 状态 1等待开奖 2已中奖 3未中奖 4撤单 5派奖回滚成功 6回滚异常的 7开奖异常
     */
    var status:Int!;
    
    
    /**
     * 购买注数
     */
    var buyZhuShu:Int!;
    
    /**
     * 中奖注数
     */
    var winZhuShu:Int!;
    
    /**
     * 购买倍数
     */
    var multiple:Int!;
    
    /**
     * 彩票期号
     */
    var qiHao:String!;
    /**
     * 玩法大类名称
     */
    var groupName:String!;// 大类名字
    /**
     * 玩法小类名称
     *
     * @return
     */
    var playName:String!;
    
    /**
     * 赔率
     */
    var peilv:String!;// 赔率,不存库
    
    /**
     * 彩种奖金
     *
     * @return
     */
    var minBonusOdds:Float!;
    
    /**
     * 盈亏输赢
     *
     * @return
     */
    var yingKui:Float!;
    
    /**
     * 模式 1元 10角 100分
     */
    var model:Int!;
    
}
